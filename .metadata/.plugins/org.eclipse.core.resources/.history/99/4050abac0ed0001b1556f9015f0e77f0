package model;

public class Dirigente extends Persona {

	private String livelloFunzionale;

	public Dirigente(int id, String nome, String cognome, String luogoDiNascita, String sesso, String codiceFiscale, String livelloFunzionale) {
		super(id, nome, cognome, luogoDiNascita, sesso, codiceFiscale);
		this.livelloFunzionale = livelloFunzionale;
	}
	
	
	
}
